{
  "name": "client",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node ./build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "npm run check -- --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "test": "vitest run",
    "start-server": "cd .. && make serve",
    "test-e2e": "playwright test",
    "test-e2e:ci": "start-server-and-test start-server '3579/docs|3000' test-e2e",
    "test:watch": "vitest watch",
    "test:coverage": "npm run test -- --coverage"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@playwright/test": "^1.27.1",
    "@sveltejs/adapter-node": "^1.0.0-next.90",
    "@sveltejs/kit": "^1.0.0-next.481",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vitest/coverage-c8": "^0.23.2",
    "cssnano": "^5.1.13",
    "dotenv": "^16.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "isomorphic-fetch": "^3.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.7.0",
    "rollup-plugin-gzip": "^3.0.1",
    "start-server-and-test": "^1.14.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "vite": "^3.1.0",
    "vitest": "^0.23.2"
  },
  "dependencies": {
    "@gouvfr/dsfr": "^1.8.4",
    "@axe-core/playwright": "^4.5.0",
    "@types/marked": "^4.0.8",
    "date-fns": "^2.28.0",
    "marked": "^4.2.4",
    "microtip": "^0.2.2",
    "svelte-forms-lib": "^2.0.1",
    "yup": "^0.32.11"
  }
}
